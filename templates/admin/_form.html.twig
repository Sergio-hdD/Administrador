{{ form_start(form) }}
    <div class="form-group row">
        <div class="col-md-6 py-2">
            {{ form_row(form.dni)}}
        </div>
        <div class="col-md-6 py-2">
            {{ form_row(form.email)}}
        </div>
        <div class="col-md-6 py-2">
            {{ form_row(form.lastname)}}
        </div>
        <div class="col-md-6 py-2">
            {{ form_row(form.name)}}
        </div>
        <div class="col-md-6 py-2">
            {{ form_row(form.phone)}}
        </div>
        <div class="col-md-6 py-2">
            {{ form_row(form.password)}}
        </div>
    </div> 
    <button class="btn btn-info w-100">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
